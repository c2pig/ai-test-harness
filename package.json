{
  "name": "ai-test-harness-testing-tools-enhanced",
  "version": "1.0.0",
  "description": "Multi-tenant AI quality assessment framework",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test:product-listing": "tsx src/index.ts --tenant=examples/product-listing",
    "test:product-listing-multimodel": "tsx src/index.ts --tenant=examples/product-listing-multimodel",
    "test:product-outreach-scenario": "tsx src/index.ts --tenant=examples/product-outreach-scenario",
    "test:product-outreach-evaluation": "tsx src/index.ts --tenant=examples/product-outreach-evaluation",
    "test:product-outreach-rerun": "tsx src/index.ts --tenant=examples/product-outreach-rerun",
    "test:product-outreach-simulation": "tsx src/index.ts --tenant=examples/product-outreach-simulation",
    "create-tenant": "tsx src/cli/create-tenant.ts",
    "validate": "tsx src/cli/validate-configs.ts && npm run type-check",
    "schema:version": "tsx src/cli/show-schema-version.ts",
    "clean:outputs": "find tenants -type d -name 'outputs' -exec rm -rf {} + && echo 'All output folders cleaned'",
    "chart": "tsx scripts/chart.ts"
  },
  "keywords": [
    "ai",
    "quality",
    "testing",
    "llm"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-bedrock-agent": "^3.918.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.600.0",
    "@aws-sdk/client-bedrock-runtime": "^3.600.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.600.0",
    "@aws-sdk/client-dynamodb": "^3.921.0",
    "@aws-sdk/client-secrets-manager": "^3.600.0",
    "@types/glob": "^8.1.0",
    "chart.js": "^4.5.1",
    "chartjs-node-canvas": "^5.0.0",
    "glob": "^11.0.3",
    "js-yaml": "^4.1.1",
    "jsonpath-plus": "^10.3.0",
    "marked": "^12.0.0",
    "pg": "^8.11.0",
    "tsx": "^4.7.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.3.0"
  }
}
